{
  "manifest_version": 2,
  "name": "social@edge-profiler",
  "short_name": "social@edge-profiler",
  "description": "host memory and cpu usage",
  "version": "0.0.1",
  "version_name": "0.0.1",
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "permissions": [
    "system.cpu",
    "system.display",
    "system.memory",
    "system.network",
    "system.storage"
  ]
}
